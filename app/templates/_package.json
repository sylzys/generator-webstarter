{
  "name": "package",
  "version": "1.0.0",
  "dependencies": {<% if (needTaskRunner) { if(taskRunner == 'gulp') {%>
  	"gulp-changed" : "latest",
  	"gulp-notify" : "latest",
  	"gulp-watch" : "latest",
  	"gulp-rename" : "latest",<% } %>
  	"<%= taskRunner %>": "latest"
  	<% if (customConfig) {%>,
  		<% l=customConfig.length;_.each(customConfig,function(k, i) { %>
  	"<%= k %>":"latest"
  	<% if (i != l-1) {%> ,
  		<% }  }) } }%>
  }
}